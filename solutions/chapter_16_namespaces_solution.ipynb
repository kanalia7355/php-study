    "    // 全ての設定を取得\n",
    "    public static function getAll(): array {\n",
    "        return [\n",
    "            'name' => self::$name,\n",
    "            'version' => self::$version,\n",
    "            'debug' => self::$debug\n",
    "        ];\n",
    "    }\n",
    "}\n",
    "\n",
 "?>"
