    "    private string $username;\n",
    "    private string $email;\n",
    "    \n",
    "    public function __construct(int $id, string $username, string $email) {\n",
    "        $this->id = $id;\n",
    "        $this->username = $username;\n",
    "        $this->email = $email;\n",
