   "source": [
    "<language>php</language><?php\n",
    "\n",
 "// PSR-4オートロードシステムの実装\n",
 "class Psr4Autoloader {\n",
    "    private array $prefixes = [];\n",
    "    \n",
    "    public function register(): void {\n",
    "        spl_autoload_register([$this, 'loadClass']);\n",
    "    }\n",
    "    \n",
    "    public function addPrefix(string $prefix, string $baseDir): void {\n",
    "        $prefix = trim($prefix, '\\\\\\') . '\\\\';\n",
    "        $baseDir = rtrim($baseDir, '/') . '/';\n",
    "        $this->prefixes[$prefix] = $baseDir;\n",
    "    }\n",
    "    \n",
    "    public function loadClass(string $class): void {\n",
    "        $prefix = $class;\n",
    "        while (($pos = strrpos($prefix, '\\\\')) !== false) {\n",
    "            $prefix = substr($prefix, 0, $pos + 1);\n",
    "            $relativeClass = substr($class, $pos + 1);\n",
    "            \n",
    "            if (isset($this->prefixes[$prefix])) {\n",
    "                $baseDir = $this->prefixes[$prefix];\n",
    "                $file = $baseDir . str_replace('\\\\', '/', $relativeClass) . '.php';\n",
    "                \n",
    "                if (file_exists($file)) {\n",
    "                    require $file;\n",
    "                    return;\n",
    "                }\n",
    "            }\n",
    "        }\n",
    "    }\n",
    "}\n",
    "\n",
 "// オートローダーの作成と登録\n",
 "$autoloader = new Psr4Autoloader();\n",
 "$autoloader->register();\n",
 "\n",
 "// PSR-4マッピング\n",
 "$autoloader->addPrefix('App', __DIR__ . '/src');\n",
 "$autoloader->addPrefix('Monolog', __DIR__ . '/vendor/monolog/src');\n",
 "$autoloader->addPrefix('Tests', __DIR__ . '/tests');\n",
    "\n",
 "?>"
   ]
