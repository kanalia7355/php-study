    "<language>php</language><?php\n",
    "\n",
 "namespace App\\Contracts;\n",
    "\n",
 "// ロガーインターフェース\n",
 "interface LoggerInterface {\n",
    "    public function log(string $message): void;\n",
    "    public function logError(string $error): void;\n",
    "}\n",
    "\n",
 "// キャッシュインターフェース\n",
 "interface CacheInterface {\n",
