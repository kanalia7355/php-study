    "        } elseif ($key === 'version') {\n",
    "            return self::$version;\n",
    "        } elseif ($key === 'debug') {\n",
    "            return self::$debug;\n",
    "        }\n",
    "        return null;\n",
    "    }\n",
    "    \n",
    "    // デバッグモードかチェック\n",
