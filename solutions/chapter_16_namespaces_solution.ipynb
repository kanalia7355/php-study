    "<language>php</language><?php\n",
    "\n",
 "// グローバル名前空間\n",
 "// 独自の日付処理関数\n",
 "function date(string $format = 'Y-m-d H:i:s'): string {\n",
    "    // グローバルなdate()関数を呼び出す\n",
    "    $timestamp = \\time();\n",
    "    return \"Custom Date: \" . \\date($format, $timestamp);\n",
    "}\n",
    "\n",
 "// 独自の文字列長処理関数\n",
 "function strlen(string $string): int {\n",
    "    // グローバルなstrlen()関数を呼び出す\n",
    "    return \\strlen(\"Length: {$string}\") - 9; // \"Length: \"を引く\n",
    "}\n",
    "\n",
 "?>"
