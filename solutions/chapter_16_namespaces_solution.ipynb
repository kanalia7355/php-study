    "            self::$version = (string)$value;\n",
    "        } elseif ($key === 'debug') {\n",
    "            self::$debug = (bool)$value;\n",
    "        }\n",
    "    }\n",
    "    \n",
    "    // 設定を取得\n",
    "    public static function get(string $key) {\n",
    "        if ($key === 'name') {\n",
